generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Portifolio {
  id               String  @id @default(uuid())
  name             String
  coin             String
  subTotal         Float?
  totalPriceActual Float?
  percent          Float?
  assets           Asset[]
}

model Asset {
  id           String      @id @default(uuid())
  price        Float
  symbol       String
  quanty       Int
  portifolio   Portifolio? @relation(fields: [portifolioId], references: [id])
  portifolioId String?
}
